{
  "icon": "docker.svg",
  "vm": {
    "composefile": "docker-compose.yaml",
    "exposes": {
      "socket": "backend.sock"
    }
  },
  "ui": {
    "dashboard-tab": {
      "title": "OpenWebUI + Model Runner",
      "src": "index.html",
      "root": "ui"
    }
  },
  "host": {
    "binaries": [
      {
        "name": "docker",
        "cmd": "docker"
      }
    ]
  },
  "capabilities": [
    "host-networking"
  ]
}
